{
  "name": "deficentral-blockchain",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "compile:clean": "npm run clean && hardhat compile",
    "coverage": "hardhat coverage",
    "clean": "rmdir artifacts\\ cache\\ typechain\\ /s /q",
    "solium": "solium --dir contracts/",
    "solium:fix": "solium --dir contracts/ --fix",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "solhint": "solhint 'contracts/**/*.sol'",
    "flatten": "hardhat flatten > flatten.sol",
    "docs": "solidity-docgen --solc-module solc-0.8.3",
    "prettier:fix": "prettier --write contracts/ && prettier --write test && prettier --write deploy && prettier --write scripts",
    "deploy:testnet:nativeRelayer": "hardhat --network testnet deploy --reset --tags WNativeRelayer",
    "deploy:mainnet:nativeRelayer": "hardhat --network mainnet deploy --reset --tags WNativeRelayer",
    "deploy:testnet:vaults": "hardhat --network testnet deploy --reset --tags Vault",
    "upgrade:testnet:vaults": "hardhat --network testnet deploy --reset --tags VaultUpgrade",
    "deploy:mainnet:vaults": "hardhat --network mainnet deploy --reset --tags Vault",
    "upgrade:mainnet:vaults": "hardhat --network mainnet deploy --reset --tags VaultUpgrade",
    "deploy:testnet:strategies:pancake": "hardhat --network testnet deploy --reset --tags PancakeswapStrategies",
    "upgrade:testnet:strategies:pancake": "hardhat --network testnet deploy --reset --tags PancakeswapStrategiesUpgrade",
    "deploy:mainnet:strategies:pancake": "hardhat --network mainnet deploy --reset --tags PancakeswapStrategies",
    "upgrade:mainnet:strategies:pancake": "hardhat --network mainnet deploy --reset --tags PancakeswapStrategiesUpgrade",
    "deploy:testnet:workers:pancake": "hardhat --network testnet deploy --reset --tags PancakeswapWorkers",
    "upgrade:testnet:workers:pancake": "hardhat --network testnet deploy --reset --tags PancakeswapWorkersUpgrade",
    "deploy:mainnet:workers:pancake": "hardhat --network mainnet deploy --reset --tags PancakeswapWorkers",
    "upgrade:mainnet:workers:pancake": "hardhat --network mainnet deploy --reset --tags PancakeswapWorkersUpgrade",
    "deploy:testnet:bountycollector": "hardhat --network testnet deploy --reset --tags BountyCollector",
    "upgrade:testnet:bountycollector": "hardhat --network testnet deploy --reset --tags BountyCollectorUpgrade",
    "deploy:mainnet:bountycollector": "hardhat --network mainnet deploy --reset --tags BountyCollector",
    "upgrade:mainnet:bountycollector": "hardhat --network mainnet deploy --reset --tags BountyCollectorUpgrade"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nextrope.com:deficentral/deficentral-blockchain.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.4.2",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.9.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "solidity-docgen": "^0.5.13",
    "solium": "^1.2.5",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@pancakeswap-libs/pancake-swap-core": "^0.1.0",
    "bn.js": "^5.2.0",
    "chai-bn": "^0.3.0",
    "moment": "^2.29.1"
  }
}
